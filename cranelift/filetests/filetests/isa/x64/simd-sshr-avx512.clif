test compile precise-output
set enable_simd
target x86_64 skylake has_avx512f has_avx512vl

function %sshr_i64x2(i64x2, i32) -> i64x2 {
block0(v0: i64x2, v1: i32):
    v2 = sshr v0, v1
    return v2
}

